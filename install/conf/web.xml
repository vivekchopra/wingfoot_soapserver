<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>
     <servlet>
       <servlet-name>wserver</servlet-name>
       <servlet-class>com.wingfoot.soap.server.HTTPListener</servlet-class>
       <load-on-startup>1</load-on-startup>
     </servlet>
     <servlet-mapping>
       <servlet-name>wserver</servlet-name>
       <url-pattern>wserver/*</url-pattern>
     </servlet-mapping>
</web-app>
